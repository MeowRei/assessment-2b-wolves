<div class="row">
    <div class="col s12 m4">
        <div class="row card blue-grey darken-1">

            <div class="card-content white-text ">
                <p>Название вечеринки: {{name}}</p>
                <p>Место вечерики: {{location}}</p>
                <p>Время вечеринки: {{time}}</p>
                <p>Организатор: {{createAt}}</p>
            </div>
        </div>
{{#if loggedin}}
        <form method="post" action="/cat/{{_id}}" class="{{_id}}">
            <a class="btn waves-effect waves-light edit" href="/party/{{ _id }}/edit">edit</a>
            <a class="btn waves-effect waves-light delete" href="/party/{{ _id }}/delete">Delete</a>
        </form>
    {{else}}
    {{/if}}
    </div>
</div>
<script src="/js/del.js"></script>
