{{#each post}}
    <div class="row">
        <div class="col s12 m4">
            <div class="row card blue-grey darken-1">

                <div class="card-content white-text {{createAt._id}}">
                    <a href="/party/{{_id}}">{{name}}</a>
                    <p>Location: {{location}}</p>
                    <p>Time: {{time}}</p>
                    <p>Owner: {{createAt.username}}</p>
                </div>
            </div>
        </div>
    </div>
{{/each}}